{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  data() {\n    return {\n      products: [{\n        name: 'Burger',\n        price: 60\n      }, {\n        name: 'Fries',\n        price: 30\n      }, {\n        name: 'Hawain Pizza',\n        price: 70\n      }, {\n        name: 'Mojos',\n        price: 80\n      }, {\n        name: 'Carbonara',\n        price: 100\n      }, {\n        name: 'Pizza Napoletana',\n        price: 150\n      }, {\n        name: 'Milk Shake',\n        price: 100\n      }, {\n        name: 'Green Tea',\n        price: 120\n      }, {\n        name: 'Orange Juice',\n        price: 50\n      }, {\n        name: 'Mango Juice',\n        price: 70\n      }],\n      cart: []\n    };\n  },\n  computed: {\n    total() {\n      return this.cart.reduce((acc, item) => acc + item.product.price * item.quantity, 0);\n    }\n  },\n  methods: {\n    addToCart(product) {\n      const existingItem = this.cart.find(item => item.product.id === product.id);\n      if (existingItem) {\n        existingItem.quantity++;\n      } else {\n        this.cart.push({\n          product: product,\n          quantity: 1\n        });\n      }\n    },\n    removeFromCart(index) {\n      this.cart.splice(index, 1);\n    },\n    updateQuantity(index) {\n      const item = this.cart[index];\n      if (item.quantity < 1) {\n        item.quantity = 1;\n      }\n    }\n  }\n};","map":{"version":3,"names":["data","products","name","price","cart","computed","total","reduce","acc","item","product","quantity","methods","addToCart","existingItem","find","id","push","removeFromCart","index","splice","updateQuantity"],"sources":["C:\\Users\\PC10\\vueplayground\\itewact3\\src\\App.vue"],"sourcesContent":["<template>\n  <div id=\"app\">\n    <h1>Vue.js Shopping Cart</h1>\n    <div class=\"products\">\n      <div class=\"product\" v-for=\"product in products\" :key=\"product.id\">\n        <span>{{ product.name }} - ₱{{ product.price }}</span>\n        <button @click=\"addToCart(product)\">Add to Cart</button>\n      </div>\n    </div>\n    <hr>\n    <div class=\"cart\">\n      <h2>Cart</h2>\n      <div class=\"cart-item\" v-for=\"(item, index) in cart\" :key=\"item.product.id\">\n        <span>{{ item.product.name }}</span>\n        <input type=\"number\" min=\"1\" v-model=\"item.quantity\" @change=\"updateQuantity(index)\">\n        <button @click=\"removeFromCart(index)\">Remove</button>\n      </div>\n      <div class=\"total\">Total: ₱{{ total }}</div>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  data() {\n    return {\n      products: [\n        { name: 'Burger', price: 60 },\n        { name: 'Fries', price: 30 },\n        { name: 'Hawain Pizza', price: 70 },\n        { name: 'Mojos', price: 80 },\n        { name: 'Carbonara', price: 100 },\n        { name: 'Pizza Napoletana', price: 150 },\n        { name: 'Milk Shake', price: 100 },\n        { name: 'Green Tea', price: 120 },\n        { name: 'Orange Juice', price: 50 },\n        { name: 'Mango Juice', price: 70 }\n      ],\n      cart: []\n    };\n  },\n  computed: {\n    total() {\n      return this.cart.reduce((acc, item) => acc + (item.product.price * item.quantity), 0);\n    }\n  },\n  methods: {\n    addToCart(product) {\n      const existingItem = this.cart.find(item => item.product.id === product.id);\n      if (existingItem) {\n        existingItem.quantity++;\n      } else {\n        this.cart.push({ product: product, quantity: 1 });\n      }\n    },\n    removeFromCart(index) {\n      this.cart.splice(index, 1);\n    },\n    updateQuantity(index) {\n      const item = this.cart[index];\n      if (item.quantity < 1) {\n        item.quantity = 1;\n      }\n    }\n  }\n};\n</script>\n\n<style scoped>\nbody {\n  font-family: Arial, sans-serif;\n  margin: 0;\n  padding: 20px;\n  background-color: #f5f5f5;\n}\n#app {\n  max-width: 800px;\n  margin: 0 auto;\n  background-color: #fff;\n  padding: 20px;\n  border-radius: 8px;\n  box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);\n}\nh1 {\n  text-align: center;\n}\n.products {\n  display: grid;\n  grid-template-columns: repeat(auto-fit, minmax(200px, 1fr));\n  gap: 10px;\n}\n.product {\n  background-color: #fff;\n  padding: 10px;\n  border-radius: 5px;\n  box-shadow: 0 2px 5px rgba(0, 0, 0, 0.1);\n}\n.product button {\n  display: block;\n  width: 100%;\n  margin-top: 10px;\n  padding: 8px 16px;\n  background-color: #007bff;\n  color: #fff;\n  border: none;\n  border-radius: 4px;\n  cursor: pointer;\n  transition: background-color 0.3s ease;\n}\n.product button:hover {\n  background-color: #0056b3;\n}\n.cart {\n  margin-top: 20px;\n}\n.cart h2 {\n  margin-bottom: 10px;\n}\n.cart-item {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  background-color: #fff;\n  padding: 10px;\n  border-radius: 5px;\n  box-shadow: 0 2px 5px rgba(0, 0, 0, 0.1);\n}\n.cart-item input[type=\"number\"] {\n  width: 50px;\n}\n.cart-item button {\n  background-color: #dc3545;\n  color: #fff;\n  border: none;\n  padding: 5px 10px;\n  border-radius: 4px;\n  cursor: pointer;\n  transition: background-color 0.3s ease;\n}\n.cart-item button:hover {\n  background-color: #c82333;\n}\n.total {\n  margin-top: 10px;\n  font-weight: bold;\n}\n</style>\n"],"mappings":";AAuBA,eAAe;EACbA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,CACR;QAAEC,IAAI,EAAE,QAAQ;QAAEC,KAAK,EAAE;MAAG,CAAC,EAC7B;QAAED,IAAI,EAAE,OAAO;QAAEC,KAAK,EAAE;MAAG,CAAC,EAC5B;QAAED,IAAI,EAAE,cAAc;QAAEC,KAAK,EAAE;MAAG,CAAC,EACnC;QAAED,IAAI,EAAE,OAAO;QAAEC,KAAK,EAAE;MAAG,CAAC,EAC5B;QAAED,IAAI,EAAE,WAAW;QAAEC,KAAK,EAAE;MAAI,CAAC,EACjC;QAAED,IAAI,EAAE,kBAAkB;QAAEC,KAAK,EAAE;MAAI,CAAC,EACxC;QAAED,IAAI,EAAE,YAAY;QAAEC,KAAK,EAAE;MAAI,CAAC,EAClC;QAAED,IAAI,EAAE,WAAW;QAAEC,KAAK,EAAE;MAAI,CAAC,EACjC;QAAED,IAAI,EAAE,cAAc;QAAEC,KAAK,EAAE;MAAG,CAAC,EACnC;QAAED,IAAI,EAAE,aAAa;QAAEC,KAAK,EAAE;MAAG,EAClC;MACDC,IAAI,EAAE;IACR,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,KAAKA,CAAA,EAAG;MACN,OAAO,IAAI,CAACF,IAAI,CAACG,MAAM,CA